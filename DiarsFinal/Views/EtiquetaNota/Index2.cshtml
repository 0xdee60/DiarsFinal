
@{
    ViewData["Title"] = "Index2";
}

<h1>Index2</h1>


<div class="container">
    @foreach (var item in ViewBag.Notas)
    {
        <h3>Titulo </h3>@item.nota.titulo
        <br />
        <h3>Contenido: </h3>@item.nota.contenido
        <br />
        <h3>Fecha de modificacion: </h3> @item.nota.fechaMod.ToShortDateString()
        <br />
        <br /><br />
    }
    <br />
    <a href="/Home/Index">Volver a mostrar todo</a>
</div>